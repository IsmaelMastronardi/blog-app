<section>
  <h1>New Post</h1>
  <%= form_with model: @new_post, url: user_posts_path(@user) do |f| %>
    <div>
      <%= f.label :title %><br>
      <%= f.text_field  :title %><br>
    </div>
    <div>
      <%= f.label :text %><br>
      <%= f.text_field :text %><br>
    </div>
    <div>
      <%= f.submit "post" %>
    </div>
  <% end %>
</section>


